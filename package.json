  },
  "devDependencies": {
    "@types/jest": "24.0.25",
    "@types/node": "12.12.24",
    "@zeit/ncc": "0.21.0",
    "jest": "24.9.0",
    "jest-circus": "24.9.0",
    "ts-jest": "24.3.0",
    "typescript": "3.7.4"
  }
